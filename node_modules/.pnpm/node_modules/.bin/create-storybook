#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/create-storybook@8.6.14/node_modules/create-storybook/bin/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/create-storybook@8.6.14/node_modules/create-storybook/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/create-storybook@8.6.14/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/create-storybook@8.6.14/node_modules/create-storybook/bin/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/create-storybook@8.6.14/node_modules/create-storybook/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/create-storybook@8.6.14/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-storybook/bin/index.cjs" "$@"
else
  exec node  "$basedir/../create-storybook/bin/index.cjs" "$@"
fi
