#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/jscodeshift@0.15.2/node_modules/jscodeshift/bin/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/jscodeshift@0.15.2/node_modules/jscodeshift/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/jscodeshift@0.15.2/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/jscodeshift@0.15.2/node_modules/jscodeshift/bin/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/jscodeshift@0.15.2/node_modules/jscodeshift/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/jscodeshift@0.15.2/node_modules:/proc/cygdrive/c/Users/antonio.ballesterosa/Anclora-version-manus/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../../bin/jscodeshift.js" "$@"
fi
